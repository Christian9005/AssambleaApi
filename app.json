{
  "name": "assambleaapi",
  "entryPoint": "AssambleaApi",
  "port": 5300,
  "domain": "asamblea.codemeshdynamics.me",
  "dotnetVersion": "10.0",
  "environment": "Production",
  "healthCheckPath": "/health",
  "healthCheckTimeout": 30,
  "projectPath": ".",
  "environmentVariables": {
    "ASPNETCORE_ENVIRONMENT": "Production",
    "ASPNETCORE_URLS": "http://0.0.0.0:5300",
    "JwtSettings__Issuer": "https://asamblea.codemeshdynamics.me",
    "JwtSettings__Audience": "https://asamblea.codemeshdynamics.me"
  },
  "secrets": [
    "ConnectionStrings__DefaultConnection",
    "JwtSettings__SecretKey"
  ]
}
